<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="kr.co.goodchoice.mapper.HostFormMapper">

	<!-- insertHouseImage -->
	<insert id="insertHouseImage" parameterType="House">
		<selectKey keyProperty="no" resultType="int" order="BEFORE">
			select house_image_seq.nextval 
			from dual
		</selectKey>
		insert into house_image (house_no, house_image_no, house_image_name)
		values (#{no}, #{housePictures})
	</insert>
	
</mapper>