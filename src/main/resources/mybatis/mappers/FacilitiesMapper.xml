<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="kr.co.goodchoice.mapper.FacilitiesMapper">

	<!-- insertHouseFacilities -->
	<insert id="insertHouseFacilities">
		insert into house_facilities (house_no, facilities_no)
		values (#{houseNo}, #{facilitiesNo})
	</insert>
	
	<!-- insertFacilities -->
	<insert id="insertFacilities">
		insert into facilities (facilities_no, facilities_type)
		value (#{facilitiesNo}, #{facilitiesType})
	</insert>
	
</mapper>