<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="kr.co.goodchoice.mapper.OftenCategoryMapper">
	
	<!-- Often getOftenById(String id); -->
	<select id="getOftenById" parameterType="string" resultType="OftenCategory">
		select often_id as id, often_type as type
		from often_question_categories
		where often_id = #{value}
	</select>
</mapper>